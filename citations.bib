@article{boisferonLanguageFamiliarityInfluences,
  title = {Language Familiarity Influences Own-Race Face Recognition in 9- and 12-Month-Old Infants},
  author = {de Boisferon, Anne Hillairet and Kubicek, Claudia and Gervain, Judit and Schwarzer, Gudrun and Loevenbruck, Hélène and Vilain, Anne and Fort, Mathilde and Méary, David and Pascalis, Olivier},
  journaltitle = {Infancy},
  volume = {n/a},
