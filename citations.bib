@article{akhtarLearningForeignLabels2012,
  title = {Learning Foreign Labels from a Foreign Speaker: The Role of (Limited) Exposure to a Second Language*},
  shorttitle = {Learning Foreign Labels from a Foreign Speaker},
  author = {Akhtar, Nameera and Menjivar, Jennifer and Hoicka, Elena and Sabbagh, Mark A.},
  date = {2012-11},
