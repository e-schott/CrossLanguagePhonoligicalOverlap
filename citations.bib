  author = {Cattani, Allegra and Abbot‐Smith, Kirsten and Farag, Rafalla and Krott, Andrea and Arreckx, Frédérique and Dennis, Ian and Floccia, Caroline},
  journaltitle = {International Journal of Language \& Communication Disorders},
