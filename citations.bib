@article{tesselNeurophysiologicalIndicesEffect2018,
  title = {Neurophysiological Indices of the Effect of Cognates on Vowel Perception in Late {{Spanish-English}} Bilinguals},
  author = {Tessel, Carol A. and Levy, Erika S. and Gitterman, Martin and Shafer, Valerie L.},
