@article{boschFirstTranslationEquivalents2014,
  title = {First Translation Equivalents in Bilingual Toddlers’ Expressive Vocabulary: {{Does}} Form Similarity Matter?},
  shorttitle = {First Translation Equivalents in Bilingual Toddlers’ Expressive Vocabulary},
  author = {Bosch, Laura and Ramon-Casas, Marta},
