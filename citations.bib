  author = {Bakker, Marjan and van Dijk, Annette and Wicherts, Jelte M.},
  options = {useprefix=true},
  date = {2012-11-01},
  journaltitle = {Perspectives on Psychological Science},
  shortjournal = {Perspect Psychol Sci},
  volume = {7},
