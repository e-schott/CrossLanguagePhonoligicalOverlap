@article{weatherheadPuttingMutualExclusivity2021,
  ids = {weatherheadPuttingMutualExclusivitya,weatherheadPuttingMutualExclusivityb},
  title = {Putting {{Mutual Exclusivity}} in {{Context}}: {{Speaker Race Influences Monolingual}} and {{Bilingual Infants}}’ {{Word-Learning Assumptions}}},
  shorttitle = {Putting {{Mutual Exclusivity}} in {{Context}}},
  author = {Weatherhead, Drew and Kandhadai, Padmapriya and Hall, D. Geoffrey and Werker, Janet F.},
  date = {2021},
  journaltitle = {Child Development},
  volume = {92},
  number = {5},
  pages = {1735--1751},
  issn = {1467-8624},
  doi = {10.1111/cdev.13626},
  url = {https://srcd.onlinelibrary.wiley.com/doi/abs/10.1111/cdev.13626},
  urldate = {2021-07-05},
  abstract = {Previous work indicates mutual exclusivity in word learning in monolingual, but not bilingual toddlers. We asked whether this difference indicates distinct conceptual biases, or instead reflects best-guess heuristic use in the absence of context. We altered word-learning contexts by manipulating whether a familiar- or unfamiliar-race speaker introduced a novel word for an object with a known category label painted in a new color. Both monolingual and bilingual infants showed mutual exclusivity for a familiar-race speaker, and relaxed mutual exclusivity and treated the novel word as a category label for an unfamiliar-race speaker. Thus, monolingual and bilingual infants have access to similar word-learning heuristics, and both use nonlinguistic social context to guide their use of the most appropriate heuristic.},
  langid = {english},
  annotation = {\_eprint: https://srcd.onlinelibrary.wiley.com/doi/pdf/10.1111/cdev.13626},
  file = {C\:\\Users\\Esther\\Zotero\\storage\\N99KQPDM\\Weatherhead et al. - Putting Mutual Exclusivity in Context Speaker Rac.pdf;C\:\\Users\\Esther\\Zotero\\storage\\9UCGAGB6\\cdev.html;C\:\\Users\\Esther\\Zotero\\storage\\MLD7E59Y\\cdev.html}
}

@article{weatherheadRoleAudiovisualSpeech2021,
  title = {The {{Role}} of {{Audiovisual Speech}} in {{Fast-Mapping}} and {{Novel Word Retention}} in {{Monolingual}} and {{Bilingual}} 24-{{Month-Olds}}},
  author = {Weatherhead, Drew and Arredondo, Maria M. and Nácar Garcia, Loreto and Werker, Janet F.},
  date = {2021-01},
  journaltitle = {Brain Sciences},
  volume = {11},
  number = {1},
  pages = {114},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/brainsci11010114},
  url = {https://www.mdpi.com/2076-3425/11/1/114},
  urldate = {2021-02-05},
  abstract = {Three experiments examined the role of audiovisual speech on 24-month-old monolingual and bilinguals\&rsquo; performance in a fast-mapping task. In all three experiments, toddlers were exposed to familiar trials which tested their knowledge of known word\&ndash;referent pairs, disambiguation trials in which novel word\&ndash;referent pairs were indirectly learned, and retention trials which probed their recognition of the newly-learned word\&ndash;referent pairs. In Experiment 1 (n = 48), lip movements were present during familiar and disambiguation trials, but not retention trials. In Experiment 2 (n = 48), lip movements were present during all three trial types. In Experiment 3 (bilinguals only, n = 24), a still face with no lip movements was present in all three trial types. While toddlers succeeded in the familiar and disambiguation trials of every experiment, success in the retention trials was only found in Experiment 2. This work suggests that the extra-linguistic support provided by lip movements improved the learning and recognition of the novel words.},
  issue = {1},
  langid = {english},
  keywords = {audiovisual speech perception,fast-mapping,word learning,word recognition},
  file = {C\:\\Users\\Esther\\Zotero\\storage\\6EI3MLNG\\Weatherhead et al. - 2021 - The Role of Audiovisual Speech in Fast-Mapping and.pdf;C\:\\Users\\Esther\\Zotero\\storage\\YHPAMNYN\\114.html}
}

