  file = {C\:\\Users\\Esther\\Zotero\\storage\\LNYSE544\\De Groot and Keijzer - 2000 - What Is Hard to Learn Is Easy to Forget The Roles.pdf;C\:\\Users\\Esther\\Zotero\\storage\\X39Z22JB\\De Groot_Keijzer_2000_What Is Hard to Learn Is Easy to Forget.pdf;C\:\\Users\\Esther\\Zotero\\storage\\P83ULVAT\\abstract.html}
}

