@article{minarOvercomingOtherraceEffect2018,
  title = {Overcoming the Other-Race Effect in Infancy with Multisensory Redundancy: 10-12-Month-Olds Discriminate Dynamic Other-Race Faces Producing Speech},
  shorttitle = {Overcoming the Other-Race Effect in Infancy with Multisensory Redundancy},
  author = {Minar, Nicholas J. and Lewkowicz, David J.},
