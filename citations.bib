  doi = {10.31219/osf.io/kp7tv},
  url = {https://osf.io/kp7tv/},
  urldate = {2022-03-24},
  abstract = {By around 12 months, infants have well-specified phonetic representations for the nouns they understand, for instance looking less at a car upon hearing ‘cur’ than ‘car’ (Swingley \& Aslin, 2002). Here we test whether such high-fidelity representations extend to irregular nouns, and regular and irregular verbs. A corpus analysis confirms the intuition that irregular verbs are far more common than irregular nouns in speech to young children. Two eyetracking experiments then test whether toddlers are sensitive to mispronunciation in regular and irregular nouns (Experiment 1) and verbs (Experiment 2). For nouns, we find both a mispronunciation and regularity effect in 18-month-olds. For verbs, in Experiment 2a, we find only a regularity effect and no mispronunciation effect in 18-month-olds, though toddlers’ poor comprehension overall limits interpretation. Finally, in Experiment 2b we find a mispronunciation effect and no regularity effect in 26-month-olds. Implications for wordform representations, lexical class, and learning are discussed.},
  langid = {american},
  keywords = {Developmental Psychology,Psychology,Social and Behavioral Sciences},
  file = {C\:\\Users\\Esther\\Zotero\\storage\\HKNJEHK5\\Moore and Bergelson - 2021 - Examining the roles of regularity and lexical clas.pdf}
}

@article{mooreWordformVariabilityInfants2021,
  title = {Wordform Variability in Infants' Language Environment and Its Effects on Early Word Learning},
  author = {Moore, Charlotte and Bergelson, Elika},
