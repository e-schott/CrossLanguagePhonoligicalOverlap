@article{dejesusBilingualChildrenSocial2017,
  title = {Bilingual Children’s Social Preferences Hinge on Accent},
  author = {DeJesus, Jasmine M. and Hwang, Hyesung G. and Dautel, Jocelyn B. and Kinzler, Katherine D.},
  date = {2017-12-01},
  journaltitle = {Journal of Experimental Child Psychology},
  shortjournal = {Journal of Experimental Child Psychology},
  volume = {164},
  pages = {178--191},
