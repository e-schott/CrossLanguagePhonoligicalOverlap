  title = {Who Can Communicate with Whom? {{Language}} Experience Affects Infants’ Evaluation of Others as Monolingual or Multilingual},
  shorttitle = {Who Can Communicate with Whom?},
  author = {Pitts, Casey E. and Onishi, Kristine H. and Vouloumanos, Athena},
  date = {2015-01},
  journaltitle = {Cognition},
  shortjournal = {Cognition},
  volume = {134},
  eprint = {25460391},
