@article{daileyLanguageInputInfants2021,
  title = {Language Input to Infants of Different Socioeconomic Statuses: {{A}} Quantitative Meta-Analysis},
  shorttitle = {Language Input to Infants of Different Socioeconomic Statuses},
  author = {Dailey, Shannon and Bergelson, Elika},
  date = {2021-02-11},
  doi = {10.31219/osf.io/jvdme},
  url = {https://osf.io/jvdme},
  urldate = {2021-02-16},
  abstract = {For the past 25 years, researchers have investigated language input to children from highsocioeconomic status and low-socioeconomic status families. Hart \& Risley (1995) first reported a ‘30 Million Word Gap’ between high-SES and low-SES infants. More recent studies (e.g. Sperry, Sperry, and Miller, 2019) have challenged the size or even existence of this gap. The present study is a quantitative meta-analysis on socioeconomic differences in language input to infants, which aims to systematically integrate decades of research on this topic. We analyzed 21 studies and found a significant effect of SES on language input quantity. However, this effect was moderated by the type of language included in language quantity measures: studies that include only child-directed speech in their language measures find a large SES difference, while studies that include all speech in a child’s environment find no effect of SES. These results support recent work suggesting that methodological decisions can affect researcher’ estimates of the word gap. Overall, we find that infants from low-SES homes heard less child-directed speech than infants from mid- to high-SES homes, though this difference was much smaller than Hart \& Risley’s ‘30 Million Word Gap.’ Finally, we underscore the need for more cross-cultural work on language development and the forces that may contribute to it, highlighting the opportunity for better integration of observational, experimental, and intervention-based approaches.},
  langid = {english},
  file = {C\:\\Users\\Esther\\Zotero\\storage\\FFDHHCW9\\Dailey and Bergelson - 2021 - Language input to infants of different socioeconom.pdf}
}

