  file = {C\:\\Users\\Esther\\Zotero\\storage\\UZZU87JF\\Delle Luche et al_2014_Differential processing of consonants and vowels in the auditory modality.pdf;C\:\\Users\\Esther\\Zotero\\storage\\EYVA3YNA\\2014-08495-003.html}
